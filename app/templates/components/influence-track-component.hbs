<div class="influence-track-component">
  {{#each track as |house|}}
  <div draggable="true"
    {{action "handleDragStart" house on="dragStart" preventDefault=false}}
    {{action "handleDragOver" on="dragOver"}}
    {{action "handleDrop" house on="drop"}}>
    &nbsp;
    {{house}}
    &nbsp;
  </div>
  {{/each}}
</div>
